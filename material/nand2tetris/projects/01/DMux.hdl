/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Nand(a=sel, b=sel, out=notsel);
    Nand(a=in, b=notsel, out=c);
    Nand(a=in, b=sel, out=d);
    Nand(a=c, b=c, out=a);
    Nand(a=d, b=d, out=b);
}
